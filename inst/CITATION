citHeader("To cite the proFIA package in publications use:")

citEntry(entry="article",  
   author=c(
   person("A.", "Delabriere"), person("U.", "Hohenester"),
   person("C.", "Junot"), person(c("E.", "A."), "Thevenot")),
   title="proFIA: Pre-processing of data from Flow Injection Analysis coupled to High-Resolution Mass Spectrometry (FIA-HRMS)",
   journal="Submitted",
   year="2016",
   volume="",
   pages="",
   url="",
   textVersion = paste("Delabriere, A., Hohenester, U., Junot, C., and Thevenot, E.A.",
                       ".",
                       "proFIA: Pre-processing of data from Flow Injection Analysis coupled to High-Resolution Mass Spectrometry (FIA-HRMS)",
                       ".",
                       ":",
                       "-."))
